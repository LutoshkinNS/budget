datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
  output   = "./../generated/prisma"
}

model User{
  id        Int      @id @default(autoincrement())
  categories Category[]
}

model Category{
  id        Int      @id @default(autoincrement())
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
  name      String
}

